      "cell_type": "code",
      "source": [
        " # Split data into training and testing sets\n",
        "y=df['Delay']\n",
        "X=df.drop('Delay',axis=1)\n",
        "X_test=test[X.columns]"
      ],
      "metadata": {
        "id": "eEQ24MEmrzXA"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Define column transformer for preprocessing\n",
        "categorical_features = ['UniqueCarrier']\n",
        "numeric_features = ['Year', 'Month', 'DayofMonth', 'DayOfWeek', 'DepTime', 'CRSDepTime', 'ArrTime', 'CRSArrTime', 'FlightNum', 'TaxiOut', 'Cancelled', 'Diverted', 'CarrierDelay', 'WeatherDelay', 'NASDelay', 'SecurityDelay', 'LateAircraftDelay']\n",
        "\n",
        "preprocessor = ColumnTransformer(\n",
        "    transformers=[\n",
        "        ('num', SimpleImputer(strategy='mean'), numeric_features),\n",
        "        ('cat', OneHotEncoder(), categorical_features)\n",
        "    ])"
      ],
      "metadata": {
        "id": "6Dqt7Uuvr1cw"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Define Gradient Boosting Classifier model\n",
        "initial_model = Pipeline(steps=[('preprocessor', preprocessor),\n",
        "                                 ('classifier', GradientBoostingClassifier(random_state=42))])"
      ],
      "metadata": {
        "id": "2fTNmQ5Gr8rT"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Handle mixed data types in categorical features\n",
        "#X_train['UniqueCarrier'] = X_train['UniqueCarrier'].astype(str)\n",
        "\n",
        "# Define preprocessing steps for numeric and categorical features\n",
        "numeric_features = ['Year', 'Month', 'DayofMonth', 'DayOfWeek', 'DepTime', 'CRSDepTime', 'ArrTime', 'CRSArrTime', 'FlightNum', 'TaxiOut', 'Cancelled', 'Diverted', 'CarrierDelay', 'WeatherDelay', 'NASDelay', 'SecurityDelay', 'LateAircraftDelay']\n",
        "categorical_features = ['UniqueCarrier']\n",
        "\n",
        "numeric_transformer = SimpleImputer(strategy='mean')\n",
        "categorical_transformer = OneHotEncoder(handle_unknown='ignore')\n",
        "\n",
        "# Combine preprocessing steps for numeric and categorical features\n",
        "preprocessor = ColumnTransformer(\n",
        "    transformers=[\n",
        "        ('num', numeric_transformer, numeric_features),\n",
        "        ('cat', categorical_transformer, categorical_features)\n",
        "    ])"
      ],
      "metadata": {
        "id": "BDBfArpGr-yB"
      },
      "execution_count": null,
      "outputs": []
    },
    {
